{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "cf-images-server",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "node",
            "env": {
                "NODE_ENV": "development",
            },
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register/transpile-only",
                "-r",
                "tsconfig-paths/register",
            ],
            "args": [
                "src/server.ts",
            ],
            "cwd": "${workspaceRoot}/server",
            "internalConsoleOptions": "openOnSessionStart",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**",
            ],
        },
    ],
}
